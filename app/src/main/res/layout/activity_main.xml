<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/et"
        android:layout_width="250dp"
        android:layout_height="106dp"
        android:layout_marginTop="56dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.44"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/camera"
        android:layout_width="161dp"
        android:layout_height="0dp"
        android:layout_marginBottom="22dp"
        android:text="camera"
        app:layout_constraintBottom_toTopOf="@+id/browser"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dial" />

    <Button
        android:id="@+id/contact_list"
        android:layout_width="153dp"
        android:layout_height="56dp"
        android:layout_marginStart="108dp"
        android:layout_marginTop="212dp"
        android:layout_marginBottom="26dp"
        android:text="Contact_list"
        app:layout_constraintBottom_toTopOf="@+id/file_manager"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/file_manager"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="11dp"
        android:layout_marginBottom="24dp"
        android:text="file_manager"
        app:layout_constraintBottom_toTopOf="@+id/dial"
        app:layout_constraintEnd_toEndOf="@+id/dial"
        app:layout_constraintStart_toStartOf="@+id/contact_list"
        app:layout_constraintTop_toBottomOf="@+id/contact_list" />

    <Button
        android:id="@+id/dial"
        android:layout_width="157dp"
        android:layout_height="0dp"
        android:layout_marginBottom="37dp"
        android:text="dial"
        app:layout_constraintBottom_toTopOf="@+id/camera"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/file_manager" />

    <Button
        android:id="@+id/browser"
        android:layout_width="169dp"
        android:layout_height="0dp"
        android:layout_marginBottom="151dp"
        android:text="Browser"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/camera" />
</androidx.constraintlayout.widget.ConstraintLayout>